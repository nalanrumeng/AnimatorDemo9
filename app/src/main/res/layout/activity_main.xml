<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <animator.jqw.com.animatordemo.MyRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <animator.jqw.com.animatordemo.MyButton
        android:id="@+id/btn_pro"
        android:text="it is PropertyAnimation !"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
        <animator.jqw.com.animatordemo.MyButton
            android:id="@+id/btn_tween"
            android:layout_below="@+id/btn_pro"
            android:layout_width="wrap_content"
            android:text="it is Tween Animation"
            android:layout_height="wrap_content" />
        <animator.jqw.com.animatordemo.MyView
            android:id="@+id/MyView2"
            android:visibility="gone"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />

    </animator.jqw.com.animatordemo.MyRelativeLayout>
</RelativeLayout>
